<div id="win-add-form">
  <%= simple_form_for(@win, :remote => true) do |f| %>
  <div class="form-inputs">
    <div class="row">
      <div class="large-4 columns"><%= f.input :lot_id %></div>
      <div class="large-4 columns"><%= f.input :buyer_id %></div>
      <div class="large-4 columns"><%= f.input :price %></div>
    </div>
  </div>
  <div class="form-control">
    <div class="row">
      <div class="large-12 columns"><%= f.button :submit, class: "btn" %><a id="win-cancel-button" class="btn">Cancel</a></div>
    </div>
  </div>
  <% end %>
</div>
